+    SPRITE "..\sprites\menu.sprite"	// 1
+    SPRITE "..\sprites\portrait.sprite" //2
+    SPRITE "..\sprites\menu2.sprite"	// 3
+    SPRITE "..\sprites\missinon_failed.sprite" // 4
